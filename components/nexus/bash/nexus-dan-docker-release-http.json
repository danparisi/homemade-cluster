{
  "name": "nexus-dan-docker-release-http",
  "online": true,
  "storage": {
    "blobStoreName": "default",
    "strictContentTypeValidation": true,
    "writePolicy": "ALLOW_ONCE",
    "latestPolicy": true
  },
  "cleanup": {
    "policyNames": [
      "weekly-cleanup"
    ]
  },
  "docker": {
    "v1Enabled": false,
    "forceBasicAuth": true,
    "httpPort": 30500,
    "httpsPort": null,
    "subdomain": null
  },
  "component": {
    "proprietaryComponents": false
  },
  "format": "docker",
  "type": "hosted"
}